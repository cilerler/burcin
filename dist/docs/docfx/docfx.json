{
	"metadata": [
		{
			"src": [
				{
					"files": [
						"src/**.csproj"
					],
					"exclude": [
						"**/bin/**",
						"**/obj/**"
					],
					"src": "../.."
				}
			],
			"dest": "../docs/docfx/api",
			"disableGitFeatures": false,
			"disableDefaultFilter": false
		}
	],
	"build": {
		"content": [
			{
				"files": [
					"api/**.yml",
					"api/index.md"
				]
			},
			{
				"files": [
					"articles/**.md",
					"articles/**/toc.yml",
					"toc.yml",
					"*.md"
				]
			}
		],
		"resource": [
			{
				"files": [
					"logo.svg",
					"favicon.ico",
					"styles/*.css",
					"styles/*.js",
					"images/**"
				],
				"exclude": [
					"_site/**",
					"**/obj/**",
					"**.meta"
				]
			}
		],
		"overwrite": [
			{
				"src": "../..",
				"files": [
					"apiSpec/*.md"
				],
				"exclude": [
					"_site/**",
					"**/obj/**",
				]
			}
		],
		"src": "../docfx",
		"dest": "_site",
		"globalMetadataFiles": [],
		"fileMetadataFiles": [],
		"template": [
			"default",
			"./templates/discordfx",
			"./plugins/DocFx.Plugins.PlantUml/template"
		],
		"postProcessors": [],
		"globalMetadata": {
			"_appName": "Burcin",
			"_appTitle": "Burcin Documentation",
			"_appFooter": "<span>© 2021 Burcin<br>Generated by <strong>DocFX</strong></span>",
			"_enableSearch": "true",
			"_gitContribute": {
				"branch": "master"
			},
			"_gitUrlPattern": "github"
		},
		"markdownEngineName": "dfm-latest",
		"noLangKeyword": false,
		"keepFileLink": false,
		"cleanupCacheHistory": false,
		"disableGitFeatures": false
	}
}
