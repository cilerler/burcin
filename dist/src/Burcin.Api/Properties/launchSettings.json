{
	"iisSettings": {
		"windowsAuthentication": false,
		"anonymousAuthentication": true,
		"iisExpress": {
			"applicationUrl": "http://localhost:44301/",
			"sslPort": 44302
		}
	},
	"$schema": "http://json.schemastore.org/launchsettings.json",
	"profiles": {
		"IIS Express": {
			"commandName": "IISExpress",
			"launchBrowser": true,
			//#if (HealthChecks)
			"launchUrl": "healthchecks-ui",
			//#else
			"launchUrl": "api/values",
			//#endif
			"use64Bit": true,
			"ancmHostingModel": "InProcess",
			"environmentVariables": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			}
		},
		"Burcin.Api": {
			"commandName": "Project",
			"launchBrowser": true,
			//#if (HealthChecks)
			"launchUrl": "health/beatpulse-ui",
			//#else
			"launchUrl": "api/values",
			//#endif
			"applicationUrl": "https://localhost:44304/;http://localhost:44303/",
			"environmentVariables": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			}
		},
		"Azure Dev Spaces": {
			"commandName": "AzureDevSpaces",
			"launchBrowser": true
		}
		//#if (DockerSupport)
		,
		"Docker": {
			"commandName": "Docker",
			"launchBrowser": true,
			"launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/api/values",
			"httpPort": 10000,
			"useSSL": true,
			"sslPort": 44305,
			"environmentVariables": {
				"ASPNETCORE_URLS": "https://+:443;http://+:80",
				"ASPNETCORE_HTTPS_PORT": "44305"
			}
		}
		//#endif
	}
}
