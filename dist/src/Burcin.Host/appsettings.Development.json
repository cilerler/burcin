{
  "Logging": {
    "IncludeScopes": true,
    "LogLevel": {
      "Default": "Trace"
    },
    "Debug": {
      "LogLevel": {
        "Default": "Trace"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Trace"
      }
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose"
    },
    "Using": [
      "Serilog.Exceptions",
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341"
        }
      }
    ],
    "Properties": {
      "Configuration": "DEVELOPMENT"
    }
  },
  "EventBus": {
   "Connection": "localhost",
   "UserName": "guest",
   "Password": "guest",
   "BrokerName": "burcin",
   "SubscriptionClientName": "events",
   "PrefetchCount": 0,
   "RetryCount": 3,
   "WaitForConfirmsOrDie": "00:00:10"
  }
}
